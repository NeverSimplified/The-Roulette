{
  "name": "The-Roulette",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "SharedObjects": {
        "$path": "src/shared"
      },
      "Packages": {
        "$path": "Packages",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "ServerCode": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "ClientCode": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "ShadowMap"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}